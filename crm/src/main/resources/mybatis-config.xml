<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!--打印日志-->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <typeAliases>
        <package name="com.study.crm.settings.domain"/>
        <package name="com.study.crm.workbench.domain"/>
    </typeAliases>
    <mappers>
        <!-- 将包内的映射器接口全部注册为映射器 -->
        <package name="com.study.crm.settings.mapper"/>
        <package name="com.study.crm.workbench.mapper"/>
    </mappers>
</configuration>
